{
  "identifier": "default",
  "description": "Default capabilities for the app",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "http:default",
    "fs:default",
    "shell:default",
    "dialog:default",
    "notification:default",
    "os:default",
    "process:default",
    "deep-link:default",
    "store:default",
    "core:event:default",
    "core:webview:allow-internal-toggle-devtools",
    "core:window:allow-close",
    "core:window:allow-minimize",
    "core:window:allow-maximize",
    "core:window:allow-unmaximize",
    "core:window:allow-is-maximized",
    "core:window:allow-start-dragging",
    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "zentrio-server",
          "sidecar": true,
          "args": true
        }
      ]
    },
    "dialog:default",
    "fs:default",
    {
      "identifier": "http:default",
      "allow": [
        { "url": "http://localhost" },
        { "url": "http://localhost:*" },
        { "url": "http://127.0.0.1" },
        { "url": "http://127.0.0.1:*" },
        { "url": "https://*" },
        { "url": "http://*" },
        { "url": "http://*.zentrio.eu" },
        { "url": "https://*.zentrio.eu" }
      ]
    },
    "notification:default",
    "notification:allow-is-permission-granted",
    "os:default",
    "process:default",
    "opener:default",
    "deep-link:default",
    "store:default",
    "safe-area-insets-css:default"
  ]
}
