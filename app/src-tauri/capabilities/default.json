{
  "identifier": "default",
  "description": "Default capabilities for the app",
  "windows": ["*"],
  "permissions": [
    "core:default",
    "core:event:default",
    "core:webview:allow-internal-toggle-devtools",
    "core:window:allow-close",
    "shell:default",
    {
      "identifier": "shell:allow-execute",
      "allow": [
        {
          "name": "zentrio-server",
          "sidecar": true,
          "args": true
        }
      ]
    },
    "dialog:default",
    "fs:default",
    "http:default",
    "notification:default",
    "os:default",
    "process:default",
    "deep-link:default"
  ]
}